<form method="post" action="{{ path('admin_pictures_delete', {'id': picture.id}) }}" onsubmit="return confirm('Etes-vous sûre de vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.id) }}">
    <button class="btn">Supprimer</button>
</form>
